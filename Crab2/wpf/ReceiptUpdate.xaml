        <Label x:Name="label1_Copy20" Content="Cost:" HorizontalAlignment="Left" Margin="571,357,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptMiscCost" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="613,356,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="125" BorderBrush="#FFEEECEC" Text="0"/>
        <Label x:Name="label1_Copy21" Content="Medical:" HorizontalAlignment="Left" Margin="338,434,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptMedical" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="436,433,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" BorderBrush="#FFEEECEC" Text="0"/>
        <Label x:Name="label1_Copy22" Content="Library:" HorizontalAlignment="Left" Margin="338,408,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptLibrary" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="436,407,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" BorderBrush="#FFEEECEC" Text="0"/>
