        <Label x:Name="label1_Copy11" Content="Total Cost :" HorizontalAlignment="Left" Margin="550,255,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptTextBookCost" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="618,254,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" BorderBrush="#FFEEECEC" Text="0"/>
        <Label x:Name="label_Copy3"  Content="Uniform" HorizontalAlignment="Left" Margin="13,295,0,0" VerticalAlignment="Top" Foreground="#FF6276F9" FontSize="16" FontFamily="Segoe UI Light"/>
