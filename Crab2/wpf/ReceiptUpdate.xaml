        <Label x:Name="label1_Copy25" Content="Passport Photos:" HorizontalAlignment="Left" Margin="338,357,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptPassportPhotos" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="436,355,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" BorderBrush="#FFEEECEC" Text="0"/>
        <Label x:Name="label1_Copy26" Content="Scouts:" HorizontalAlignment="Left" Margin="338,461,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptScouts" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="436,460,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" BorderBrush="#FFEEECEC" Text="0"/>
        <Label x:Name="label1_Copy23" Content="Role:" HorizontalAlignment="Left" Margin="571,328,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ReceiptMiscRole" HorizontalAlignment="Left" VerticalContentAlignment="Bottom" Height="23" Margin="613,327,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="125" BorderBrush="#FFEEECEC" Text="Role.."/>
        <Label x:Name="label_Copy5"  Content="Miscellaneous" HorizontalAlignment="Left" Margin="571,291,0,0" VerticalAlignment="Top" Foreground="#FF6276F9" FontSize="16" FontFamily="Segoe UI Light"/>
        <Button x:Name="button" Click="SaveButton_Click" Content="SAVE" HorizontalAlignment="Left" Margin="671,500,0,0" VerticalAlignment="Top" Width="93" Height="42" BorderBrush="{x:Null}" FontSize="16" Foreground="#FFC31B1B"/>
