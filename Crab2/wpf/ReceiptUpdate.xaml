        <Label x:Name="label1_Copy1" Content="Date:" HorizontalAlignment="Left" Margin="421,43,0,0" VerticalAlignment="Top"/>
        <DatePicker HorizontalAlignment="Left" SelectedDateFormat="Short"  Name="ReceiptDate" Margin="463,43,0,0" VerticalAlignment="Top" BorderBrush="#FFEEECEC"/>
        <Label x:Name="label_Copy1"  Content="Examination" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" Foreground="#FF6276F9" FontSize="16" FontFamily="Segoe UI Light"/>
        <Label x:Name="label1_Copy2" Content="Tuition:" HorizontalAlignment="Left" Margin="10,129,0,0" VerticalAlignment="Top"/>
