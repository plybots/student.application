                        <TextBox Name="SearchName" CharacterCasing="Upper" Visibility="Collapsed" Margin="5,0,0,0" Text="Student Name" Width="187" Foreground="#FF303232" Background="{x:Null}" FontFamily="Segoe UI Light" BorderBrush="#FFA9ADAD" />

                        <TextBox Name="SearchNo" Visibility="Collapsed" Text="Student Number" Margin="5,0,0,0" Width="151" Background="{x:Null}" Foreground="#FF303232" FontFamily="Segoe UI Light" BorderBrush="#FFA9ADAD" />

                        <ComboBox Name="SearchClass" Visibility="Collapsed" Loaded="SearchClass_Loaded" Margin="5,0,0,0" Width="98" Background="#FFF0F0F0" Foreground="#FF303232" FontFamily="Segoe UI Light" BorderBrush="#FFA9ADAD"/>

                        <ComboBox Name="SearchOffering" Visibility="Collapsed" Loaded="SearchOffering_Loaded" Margin="5,0,0,0" Width="128" Background="#FFF0F0F0" Foreground="#FF303232" FontFamily="Segoe UI Light" BorderBrush="#FFA9ADAD" />

                        <ComboBox Name="SearchSubjects" Visibility="Collapsed" Loaded="SearchSubjects_Loaded" Margin="5,0,0,0" Width="113" Background="#FFF0F0F0" Foreground="#FF303232" FontFamily="Segoe UI Light" BorderBrush="#FFA9ADAD" />

                        <ComboBox Name="SearchSponsor" Visibility="Collapsed" ItemsSource="{Binding Sponsors}" Margin="5,0,0,0" Width="117" Background="#FFF0F0F0" Foreground="#FF303232" FontFamily="Segoe UI Light" BorderBrush="#FFA9ADAD">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding Name}" />
                                    </StackPanel>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>

                        <Label Name="ttballabel" Visibility="Collapsed" Content="With Tuition Balance:" Background="{x:Null}" Foreground="#FF303232" FontFamily="Segoe UI Light"/>
