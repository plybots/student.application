<Window x:Class="CRABSTUDENT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:CRABSTUDENT"
        mc:Ignorable="d"
        Title="CRAB.STUDENT" Height="608" Width="1163.775" MinWidth="1000">
    <Window.Resources>
        <Style x:Key="SigninButton" TargetType="Button">
            <Setter Property="BorderThickness" Value="0"/>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="AliceBlue"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <Grid Background="#FF2BB673">
        <!--main content-->
        <Grid Name="MainContent" Visibility="Visible" Background="#FFE7E8EC">
            <!--start of list view-->
            <Grid  Name="StudentScrollViewer" Visibility="Visible" Background="#FFF0F0F0">
                <Grid Background="White" Margin="0,60,0,0" HorizontalAlignment="Stretch">
                    <Grid HorizontalAlignment="Stretch">
                        <ListView Background="#FFF0F0F0" x:Name="StudentsList" SelectionChanged="StudentsList_SelectionChanged" ItemsSource="{Binding SelectedStudentsList}" BorderBrush="{x:Null}" Margin="0,0,-1,0"  HorizontalAlignment="Stretch">

                            <ListView.Resources>
                                <sys:Double x:Key="{x:Static SystemParameters.VerticalScrollBarWidthKey}">10</sys:Double>
                                <sys:Double x:Key="{x:Static SystemParameters.HorizontalScrollBarHeightKey}">10</sys:Double>
                                <Style TargetType="{x:Type GridViewColumnHeader}">
                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                    <Setter Property="Padding" Value="8,5,8,5"/>
                                    <Setter Property="Foreground" Value="#FF4D4D4D" />
                                    <Setter Property="Background" Value="#FFEEEEF2" />
                                    <Setter Property="FontFamily" Value="Segoe UI Semilight" />
                                    <Setter Property="FontSize" Value="11" />
                                </Style>
                                <Style TargetType="{x:Type ListView}">
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="BorderThickness" Value="0" />
                                        </Trigger>
                                        <Trigger Property="Selector.IsSelected" Value="True">
                                            <Setter Property="BorderThickness" Value="0" />
                                        </Trigger>
                                        <Trigger Property="IsMouseOver" Value="False">
                                            <Setter Property="BorderThickness" Value="0" />
                                        </Trigger>
                                        <Trigger Property="Selector.IsSelected" Value="False">
                                            <Setter Property="BorderThickness" Value="0" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </ListView.Resources>
                            <ListView.ItemContainerStyle>
                                <Style TargetType="{x:Type ListViewItem}">
                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                    <Setter Property="Foreground" Value="#FF535050"/>
                                    <Setter Property="Background" Value="#FFE7E8EC" />
                                    <Setter Property="FontFamily" Value="Segoe UI Symbol" />
                                    <Setter Property="FontSize" Value="12" />
                                    <EventSetter Event="Control.MouseDoubleClick" Handler="MenuReadPaymentOption_Click" />
                                    <Setter Property="Padding" Value="5,8"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="BorderThickness" Value="0" />
                                            <Setter Property="Background" Value="#FFEEEEF2"/>
                                            <Setter Property="Effect" Value="{x:Null}" />
                                            <Setter Property="Foreground" Value="#FF007ACC"/>
                                        </Trigger>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter Property="BorderThickness" Value="0" />
                                            <Setter Property="Background" Value="#FFEEEEF2"/>
                                            <Setter Property="Effect" Value="{x:Null}" />
                                            <Setter Property="Foreground" Value="#FF007ACC"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </ListView.ItemContainerStyle>

                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="#" Width="50" DisplayMemberBinding="{Binding Count}" />
                                    <GridViewColumn Header="NAME" Width="250" DisplayMemberBinding="{Binding Student_name}" />
                                    <GridViewColumn Header="NUMBER" Width="80" DisplayMemberBinding="{Binding Student_no}" />
                                    <GridViewColumn Header="CLASS" Width="80" DisplayMemberBinding="{Binding Student_class}" />
                                    <GridViewColumn Header="OFFERING" Width="150" DisplayMemberBinding="{Binding Offering}"  />
                                    <GridViewColumn Header="SUBJECTS" Width="140" DisplayMemberBinding="{Binding Subjects}" />
                                    <GridViewColumn Header="SPONSOR" Width="180" DisplayMemberBinding="{Binding SponsorName}" />
                                    <GridViewColumn Header="REGISTERED ON" Width="120" DisplayMemberBinding="{Binding Date_created}" />
                                    <GridViewColumn Header="UPDATED ON" Width="120" DisplayMemberBinding="{Binding Last_modified}" />
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </Grid>

                </Grid>
            </Grid>
            <!--/end of list view-->

            <!--registration-->
            <Grid Background="White" Margin="0,49,0,1" Visibility="Collapsed" Name="RegistrationGrid">
                <Grid Background="Beige" Width="440" HorizontalAlignment="Left" Margin="10,0,0,10">
                    <Label x:Name="label"  Content="Student Registration" HorizontalAlignment="Left" Margin="10,27,0,0" VerticalAlignment="Top" Foreground="#FF6A0202" FontSize="24"/>
                    <Rectangle Fill="#FFCECED3" HorizontalAlignment="Left" Height="1" Margin="14,68,0,0" VerticalAlignment="Top" Width="246" RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform ScaleY="-1"/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Rectangle.RenderTransform>
                    </Rectangle>
                    <TextBox x:Name="StudentNoTextBox" HorizontalAlignment="Left" Height="23" Margin="122,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="245" VerticalContentAlignment="Center" TextAlignment="Left"/>
                    <Label x:Name="StudentNoLabel" Content="Student Number" HorizontalAlignment="Left" Margin="19,78,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <TextBox x:Name="StudentNameTextBox" CharacterCasing="Upper" HorizontalAlignment="Left" Height="23" Margin="122,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="245" VerticalContentAlignment="Center" TextAlignment="Left"/>
                    <Label x:Name="StudentNameLabel" Content="Student Name" HorizontalAlignment="Left" Margin="19,106,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light"/>
                    <ComboBox x:Name="StudentClassComboBox" SelectionChanged="StudentClassComboBox_SelectionChanged" HorizontalAlignment="Left" Height="23" Margin="122,137,0,0" VerticalAlignment="Top" Width="245" VerticalContentAlignment="Center"/>
                    <Label x:Name="StudentClassLabel" Content="Student Class" HorizontalAlignment="Left" Margin="19,135,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <ComboBox x:Name="SponsorComboBox" ItemsSource="{Binding Sponsors}" HorizontalAlignment="Left" Height="23" Margin="122,165,0,0" VerticalAlignment="Top" Width="245" VerticalContentAlignment="Center">
                        <ComboBox.ItemTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="{Binding Name}" />
                                </StackPanel>
                            </DataTemplate>
                        </ComboBox.ItemTemplate>
                    </ComboBox>
                    <Label x:Name="feesBalanceLabel" Content="Sponsor" HorizontalAlignment="Left" Margin="19,162,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <Label x:Name="notesLabel_Copy1" Content="Subjects" HorizontalAlignment="Left" Margin="18,189,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <ComboBox x:Name="SubjectsComboBox" ItemsSource="{Binding subjects}" HorizontalAlignment="Left" Margin="122,193,0,0" VerticalAlignment="Top" Width="245"/>
                    <TextBox x:Name="StudentFeesOffer" HorizontalAlignment="Left" Height="23" Margin="122,261,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="245" VerticalContentAlignment="Center" TextAlignment="Left" Text="0"/>
                    <Label x:Name="StudentNameLabel_Copy" Content="Fees Offer" HorizontalAlignment="Left" Margin="19,258,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light"/>
                    <CheckBox x:Name="StudentFeesHasComputer" Content="( Check Where Applicable )" HorizontalAlignment="Left" Height="23" Margin="122,228,0,0" VerticalAlignment="Top" Width="245" VerticalContentAlignment="Center"/>
                    <Label x:Name="StudentNameLabel_Copy1" Content="Computer" HorizontalAlignment="Left" Margin="19,225,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light"/>
                    <Button x:Name="NewStudentSaveButton" Content="save" Click="MenuSaveButton_Click" HorizontalAlignment="Left" Margin="292,306,0,0" VerticalAlignment="Top" Width="75" Height="28" IsDefault="True" Cursor="Pen" BorderBrush="{x:Null}" Foreground="#FFF00E0E" FontSize="14.667" FontFamily="Segoe UI Semilight"/>
                </Grid>
                <Grid Background="Bisque" Margin="455,0,10,10">
                    <Label x:Name="label_Copy"  Content="Registration Results" HorizontalAlignment="Left" Margin="10,27,0,0" VerticalAlignment="Top" Foreground="#FF6A0202" FontSize="18.667" FontFamily="Segoe UI Light"/>
                    <Rectangle Fill="#FFCECED3" HorizontalAlignment="Left" Height="1" Margin="12,60,0,0" VerticalAlignment="Top" Width="246" RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform ScaleY="-1"/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Rectangle.RenderTransform>
                    </Rectangle>
                    <Label x:Name="StudentNoReg" HorizontalAlignment="Left" Content="n/a" Height="23" VerticalAlignment="Top" VerticalContentAlignment="Center" Margin="112,74,0,0" RenderTransformOrigin="0.522,-0.13" Width="155" />
                    <Label x:Name="label40" Content="Student Number" HorizontalAlignment="Left" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light" Margin="18,74,0,0"/>
                    <Label x:Name="StudentNameReg" HorizontalAlignment="Left" Height="23" Margin="112,106,0,0" VerticalAlignment="Top" Width="424" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label41" Content="Student Name" HorizontalAlignment="Left" Margin="19,106,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light" Width="93"/>
                    <Label x:Name="StudentClassReg" HorizontalAlignment="Left" Height="23" Margin="112,135,0,0" VerticalAlignment="Top" Width="152" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label42" Content="Student Class" HorizontalAlignment="Left" Margin="19,135,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light" Width="93"/>
                    <Label x:Name="StudentSponsorReg" HorizontalAlignment="Left" Height="23" Margin="112,162,0,0" VerticalAlignment="Top" Width="152" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label43" Content="Sponsor" HorizontalAlignment="Left" Margin="19,162,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light" Width="93"/>
                    <Label x:Name="label45" Content="Subjects" HorizontalAlignment="Left" Margin="18,186,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light" Width="94"/>
                    <Label x:Name="SubjectsReg" HorizontalAlignment="Left" Margin="112,186,0,0" VerticalAlignment="Top" Width="155" Height="25" Content="n/a"/>
                    <Label x:Name="StudentFeesOfferReg" HorizontalAlignment="Left" Height="23" Margin="112,252,0,0" VerticalAlignment="Top" Width="155" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label41_Copy" Content="Fees Offer" HorizontalAlignment="Left" Margin="19,252,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light" Width="93"/>
                </Grid>
            </Grid>
            <!--/end of registration-->

            <!--payments grid-->
            <Grid Name="PaymentsGrid" Visibility="Collapsed" Margin="10,49,0,1">
                <Grid Width="300"  Background="AntiqueWhite" Margin="0,0,0,10" HorizontalAlignment="Left">
                    <Label x:Name="StudentInformation" MouseDoubleClick="StudentInformation_MouseDoubleClick" Content="Student Information" HorizontalAlignment="Left" Margin="13,27,0,0" VerticalAlignment="Top" Foreground="#FF6A0202" FontSize="24" FontFamily="Segoe UI Light"/>
                    <Rectangle Fill="#FFCECED3" HorizontalAlignment="Left" Height="1" Margin="18,68,0,0" VerticalAlignment="Top" Width="246" RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform ScaleY="-1"/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Rectangle.RenderTransform>
                    </Rectangle>
                    <Label x:Name="StudentNoPay" HorizontalAlignment="Left" Content="n/a" Height="23" VerticalAlignment="Top" VerticalContentAlignment="Center" Margin="112,74,0,0" RenderTransformOrigin="0.522,-0.13" Width="155" />
                    <Label x:Name="label10" Content="Student Number" HorizontalAlignment="Left" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light" Margin="18,74,0,0"/>
                    <Label x:Name="StudentNamePay" HorizontalAlignment="Left" Height="23" Margin="112,106,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label12" Content="Student Name" HorizontalAlignment="Left" Margin="19,106,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light"/>
                    <Label x:Name="StudentClassPay" HorizontalAlignment="Left" Height="23" Margin="112,135,0,0" VerticalAlignment="Top" Width="152" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label21" Content="Student Class" HorizontalAlignment="Left" Margin="19,135,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <Label x:Name="StudentSponsorPay" HorizontalAlignment="Left" Height="26" Margin="112,162,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label22" Content="Sponsor" HorizontalAlignment="Left" Margin="19,162,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <Label x:Name="label23" Content="Offering Type" HorizontalAlignment="Left" Margin="18,189,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <Label x:Name="offeringTypePay" HorizontalAlignment="Left" Margin="112,189,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="155" Height="26" Content="n/a"/>
                    <Label x:Name="label24" Content="Subjects" HorizontalAlignment="Left" Margin="18,216,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center" RenderTransformOrigin="0.443,0.301" FontFamily="Yu Gothic UI Light"/>
                    <Label x:Name="SubjectsPay" HorizontalAlignment="Left" Margin="112,216,0,0" VerticalAlignment="Top" Width="155" Height="25" Content="n/a"/>
                    <Label x:Name="StudentFeesOfferPay" HorizontalAlignment="Left" Height="23" Margin="111,278,0,0" VerticalAlignment="Top" Width="155" VerticalContentAlignment="Center" Content="n/a"/>
                    <Label x:Name="label12_Copy" Content="Fees Offer" HorizontalAlignment="Left" Margin="18,278,0,0" VerticalContentAlignment="Center"   VerticalAlignment="Top" RenderTransformOrigin="0.427,0.077" FontFamily="Yu Gothic UI Light"/>
                </Grid>
                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="305,0,255,10" Background="Bisque">
                    <ScrollViewer.Resources>
                        <sys:Double x:Key="{x:Static SystemParameters.VerticalScrollBarWidthKey}">6</sys:Double>
                        <sys:Double x:Key="{x:Static SystemParameters.HorizontalScrollBarHeightKey}">6</sys:Double>
                    </ScrollViewer.Resources>
                    <Grid Width="1100">
                        <Label x:Name="label2_Copy1" Content="Tuition" HorizontalAlignment="Left" Margin="14,74,0,0" VerticalAlignment="Top" Foreground="#FF1CA9C9" FontSize="18.667" FontFamily="Segoe UI Light" Background="{x:Null}"/>
                        <Label x:Name="label2_Copy" Content="Student Payments" HorizontalAlignment="Left" Margin="14,22,0,0" VerticalAlignment="Top" Foreground="#FF6A0202" FontSize="24" FontFamily="Segoe UI Light"/>
                        <Rectangle Fill="#FFCECED3" HorizontalAlignment="Left" Height="1" Margin="18,63,0,0" VerticalAlignment="Top" Width="231" RenderTransformOrigin="0.5,0.5">
                            <Rectangle.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleY="-1"/>
                                    <SkewTransform/>
                                    <RotateTransform/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                        <TextBlock x:Name="StudentTuition" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="103,118,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="53"  RenderTransformOrigin="0.5,0.5">
                            <TextBlock.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-0.422"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </TextBlock.RenderTransform>
                        </TextBlock>
                        <Label x:Name="label8" Content="Tuition:" HorizontalAlignment="Left" Margin="14,114,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentAdmission" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="103,148,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="51" />
                        <Label x:Name="label8_Copy" Content="Admission:" HorizontalAlignment="Left" Margin="14,144,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentComputer" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="103,177,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="51" />
                        <Label x:Name="label8_Copy1" Content="Computer:" HorizontalAlignment="Left" Margin="14,173,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentDevelopment" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="103,207,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="51" />
                        <Label x:Name="label8_Copy2" Content="Development:" HorizontalAlignment="Left" Margin="14,203,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="label2_Copy3" Content="Exams" HorizontalAlignment="Left" Margin="320,74,0,0" VerticalAlignment="Top" Foreground="#FF1CA9C9" FontSize="18.667" FontFamily="Segoe UI Light" Background="{x:Null}"/>
                        <TextBlock x:Name="StudentExamsUACE" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="373,118,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="72" />
                        <Label x:Name="label8_Copy3" Content="UACE:" HorizontalAlignment="Left" Margin="320,114,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentExamsUCE" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="373,148,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="72" />
                        <Label x:Name="label8_Copy4" Content="UCE:" HorizontalAlignment="Left" Margin="320,144,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentExamsMock" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="373,177,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="72" />
                        <Label x:Name="label8_Copy5" Content="MOCK:" HorizontalAlignment="Left" Margin="320,173,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="label2_Copy4" Content="TextBooks" HorizontalAlignment="Left" Margin="14,249,0,0" VerticalAlignment="Top" Foreground="#FF1CA9C9" FontSize="18.667" FontFamily="Segoe UI Light" Background="{x:Null}"/>
                        <TextBlock x:Name="StudentTextBookName" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="76,293,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy6" Content="Name (s):" HorizontalAlignment="Left" Margin="14,289,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentTextBookRole" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="76,323,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy7" Content="Role (s):" HorizontalAlignment="Left" Margin="14,319,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentTextBookCost" IsEnabled="False" Foreground="Black" Background="{x:Null}"  HorizontalAlignment="Left" Height="23" Margin="76,352,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy8" Content="Total Cost:" HorizontalAlignment="Left" Margin="14,348,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="label2_Copy5" Content="Uniform" HorizontalAlignment="Left" Margin="644,71,0,0" VerticalAlignment="Top" Foreground="#FF1CA9C9" FontSize="18.667" FontFamily="Segoe UI Light" Background="{x:Null}"/>
                        <TextBlock x:Name="StudentClassUniform" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="733,115,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy9" Content="Class Uniform:" HorizontalAlignment="Left" Margin="644,111,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentCasualWear" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="733,145,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy10" Content="Casual Wear:" HorizontalAlignment="Left" Margin="644,141,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentSportsWear" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="733,174,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy11" Content="Sports Wear:" HorizontalAlignment="Left" Margin="644,170,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentSweater" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="733,204,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy12" Content="Sweater:" HorizontalAlignment="Left" Margin="644,200,0,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentBadge" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="733,297,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy13" Content="Badge:" HorizontalAlignment="Left" Margin="644,293,0,0" VerticalAlignment="Top"/>
