                        <TextBlock x:Name="StudentIDCard" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="964,114,-181,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy16" Content="Identity Card:" HorizontalAlignment="Left" Margin="875,110,-53,0" VerticalAlignment="Top"/>
                        <TextBlock x:Name="StudentPassportPhoto" IsEnabled="False" Foreground="Black" Background="Bisque"  HorizontalAlignment="Left" Height="23" Margin="964,144,-181,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" />
                        <Label x:Name="label8_Copy17" Content="Passport Photo:" HorizontalAlignment="Left" Margin="875,140,-64,0" VerticalAlignment="Top"/>
