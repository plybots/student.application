                            <Grid Margin="0,4,10,-4" Name="SearchBalanceOptions">
                                <RadioButton GroupName="Balances" x:Name="SearchWithBalance" VerticalAlignment="Center" Content="Yes" Foreground="#FF303232" FontFamily="Segoe UI Light"/>
                                <RadioButton GroupName="Balances" x:Name="SearchNoBalance" VerticalAlignment="Center" Content="No" Margin="40,0,0,0" Foreground="#FF303232" FontFamily="Segoe UI Light"/>
                                <RadioButton GroupName="Balances" IsChecked="True" x:Name="SearchAnyBalance" VerticalAlignment="Center" Content="Any" Margin="80,0,0,0" Foreground="#FF303232" FontFamily="Segoe UI Light"/>
                            </Grid>
                        </GroupBox>

